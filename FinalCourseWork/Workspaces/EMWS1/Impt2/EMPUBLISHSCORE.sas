*;
*MOST FREQUENT VALUE (COUNT);
*;
length IMP_GENDER $1;
format IMP_GENDER $1.;
label IMP_GENDER = 'Imputed GENDER';
IMP_GENDER = GENDER;
if GENDER = '' then IMP_GENDER = 'F';
length IMP_NGROUP $1;
format IMP_NGROUP $1.;
label IMP_NGROUP = 'Imputed NGROUP';
IMP_NGROUP = NGROUP;
if NGROUP = '' then IMP_NGROUP = 'C';
length IMP_OAC 8;
format IMP_OAC BEST12.0;
label IMP_OAC = 'Imputed OAC';
IMP_OAC = OAC;
if missing(OAC) then IMP_OAC = 2;
length IMP_REGION $10;
format IMP_REGION $10.;
label IMP_REGION = 'Imputed REGION';
IMP_REGION = REGION;
if REGION = '' then IMP_REGION = 'South East';
length IMP_TV_REG $12;
format IMP_TV_REG $12.;
label IMP_TV_REG = 'Imputed TV_REG';
IMP_TV_REG = TV_REG;
if TV_REG = '' then IMP_TV_REG = 'London';
